{
  "name": "eup-cups",
  "version": "0.0.1",
  "description": "TypeScript IPP client implementation with CUPS helpers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:real": "CUPS_URL=http://localhost:631 jest tests/integration/cups-real.test.ts",
    "test:real:skip-real": "SKIP_CUPS_REAL=1 jest tests/integration/cups-real.test.ts",
    "test:ipp": "CUPS_URL=http://localhost:631 jest tests/ipp/ops/",
    "test:ipp:skip-real": "SKIP_IPP_REAL=1 jest tests/ipp/ops/",
    "test:cups": "CUPS_URL=http://localhost:631 jest tests/cups/ops/",
    "test:cups:skip-real": "SKIP_CUPS_REAL=1 jest tests/cups/ops/"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^22.10.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/tests"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/examples/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 89,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "eup-ipp-encoder": "0.0.1"
  }
}
